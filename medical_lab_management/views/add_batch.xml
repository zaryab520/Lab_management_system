<odoo>
 



<record model="ir.ui.view" id="test_module_tree">
      <field name="name">lab batch tree</field>
      <field name="model">lab.batch</field>
      <field name="arch" type="xml">

        <tree>
			<field name="batch_name"/>
					
			<field name="test_record"/>
		</tree>

      </field>
    </record>





	 <record model="ir.ui.view" id="test_batch_form">
      <field name="name">lab batch form</field>
      <field name="model">lab.batch</field>
      <field name="arch" type="xml">

      <form>
        <sheet>
          <group>
            <field name="batch_name"/>
           <field name="test_record"/>
          </group>
        </sheet> 


      </form>
      </field>
    </record>


<record model="ir.actions.act_window" id="action_any_name_goes_here">
	<field name="name">My Action</field>
	<field name="res_model">lab.batch</field>
	<!-- <field name="view_type">form</field> -->
	<field name="view_mode">tree,form</field>
	<!-- <field name="target">new</field> -->
</record>



<menuitem id="lab_batch_wizard" name="Create Batch" sequence="20"
        parent="menu_root_lab" action="action_any_name_goes_here"
        groups="group_lab_management_user,group_lab_management_technician"/>




</odoo>